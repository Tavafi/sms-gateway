{
  "name": "cp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:modern": "nuxt --modern=client",
    "build": "nuxt build",
    "build:modern": "nuxt build --modern=client",
    "start": "nuxt start",
    "start:modern": "nuxt start --modern=client",
    "generate": "nuxt generate",
    "generate:modern": "nuxt generate --modern=client",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "dependencies": {
    "core-js": "^3",
    "nuxt": "^2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5",
    "@nuxtjs/eslint-module": "^3",
    "@nuxtjs/stylelint-module": "^4",
    "@nuxtjs/vuetify": "^1",
    "babel-eslint": "^10",
    "eslint": "^7",
    "eslint-config-prettier": "^7",
    "eslint-plugin-nuxt": "^2",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-vue": "^7",
    "prettier": "^2",
    "stylelint": "^13",
    "stylelint-config-prettier": "^8",
    "stylelint-config-standard": "^20"
  }
}
